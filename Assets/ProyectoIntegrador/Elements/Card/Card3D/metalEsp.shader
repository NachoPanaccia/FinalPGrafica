			float2 uv_TextureSample0 = i.uv_texcoord * _TextureSample0_ST.xy + _TextureSample0_ST.zw;
			o.Smoothness = ( 0.9 + ( abs( sin( ( tex2D( _TextureSample0, uv_TextureSample0 ) * float4( float2( 1,40 ), 0.0 , 0.0 ) ) ) ) * 0.1 ) ).r;
